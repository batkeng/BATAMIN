{
  "compilerOptions": {
    "typeRoots": ["./@types", "./node_modules/@types"],
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "baseUrl": "src",
    "paths": {
      "apis/*": ["apis/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"],
      "pages/*": ["pages/*"],
      "types/*": ["types/*"],
      "utils/*": ["utils/*"]
    }
  },
  "files": ["src/types/types.ts"]
}
